  "optionalDependencies": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie < 12",
      "Safari 10",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "./.eslintrc.js"
    ]
  }
