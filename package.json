  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "postinstall": "npm run i18n",
    "start": "env-cmd -f ./.env.development react-app-rewired start",
    "build": "env-cmd -f ./.env.staging react-app-rewired build -- --stats && cp -f CNAME ./build/ && npm run patchsw",
    "build:prod": "mkdir -p ./build && env-cmd -f ./.env.production react-app-rewired build && cp -f CNAME.PROD ./build/CNAME && rm build/robot.txt && npm run patchsw && npm run postbuild",
    "build:mixer": "mkdir -p ./build && env-cmd -f ./.env.mixer.production react-app-rewired build && cp -f CNAME.PROD ./build/CNAME && rm build/robot.txt && npm run patchsw && npm run postbuild",
    "postbuild": "",
    "postbuild:snap": "react-snap",
    "test": "env-cmd -f ./.env.development.local react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "proxy": "node proxy.js",
    "deploy": "npm run build && npm run deploy2",
    "deploy2": "env-cmd -f ./.env.staging netlify deploy --prod --dir=build",
    "deploy:prod": "npm run build:prod && env-cmd -f ./.env.production netlify deploy --prod --dir=build",
    "deploy:mixer": "npm run build:mixer && env-cmd -f ./.env.mixer.production netlify deploy --prod --dir=build",
    "i18n": "gstoi18n 1R85easxOUiw184YPJ3wI2-oC-yPyKZi_Xn6a38foQFc ./public/i18n/ -w 0 -w 1 -w 2",
    "patchsw": "cat ./sw.js.append >> ./build/service-worker.js",
    "old_patchsq": "sed -i.bak '/workbox.clientsClaim();/i workbox.skipWaiting();' ./build/service-worker.js && rm ./build/service-worker.js.bak"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
