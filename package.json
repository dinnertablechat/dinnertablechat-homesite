  "scripts": {
    "postinstall": "npm run i18n",
    "start": "react-scripts start",
    "build": "env-cmd ./.env.staging react-scripts build && cp -f CNAME ./build/ && npm run i18n",
    "build:prod": "react-scripts build && cp -f CNAME.PROD ./build/CNAME && npm run i18n && rm build/robot.txt",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "proxy": "node proxy.js",
    "predeploy": "mkdir -p ./build",
    "deploy": "npm run build && gh-pages -d build",
    "deploy:prod": "npm run predeploy && npm run build:prod && gh-pages -d build -o prod",
    "i18n": "gstoi18n 1R85easxOUiw184YPJ3wI2-oC-yPyKZi_Xn6a38foQFc ./public/i18n/"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
