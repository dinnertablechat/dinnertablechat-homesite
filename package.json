  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "aws-amplify": "^1.0.8",
    "aws-amplify-react": "^2.0.0",
    "aws-sdk": "^2.307.0",
    "env-cmd": "^8.0.2",
    "hark": "^1.2.3",
    "history": "^4.7.2",
    "mobx": "^5.1.0",
