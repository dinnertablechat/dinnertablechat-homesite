  "scripts": {
    "start": "react-scripts-ts start",
    "build": "env-cmd ./.env.staging react-scripts-ts build && cp -f CNAME ./build/",
    "build:prod": "react-scripts-ts build && cp -f CNAME.PROD ./build/CNAME",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "proxy": "node proxy.js",
    "predeploy": "mkdir -p ./build",
    "deploy": "npm run build && gh-pages -d build",
    "deploy:prod": "npm run predeploy && npm run build:prod && gh-pages -d build"
  },
