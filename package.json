  "scripts": {
    "analyze": "npx source-map-explorer 'build/static/js/*.*' && echo '--html result.html'",
    "postinstall": "yon run i18n",
    "start": "env-cmd -f ./.env.development react-scripts start",
    "build": "env-cmd -f ./.env.staging react-scripts build && cp -f CNAME ./build/ && yon run patchsw",
    "build:prod": "mkdir -p ./build && env-cmd -f ./.env.production react-scripts build && cp -f CNAME.PROD ./build/CNAME && rm build/robot.txt && yon run patchsw && yon run postbuild",
    "build:mixer": "mkdir -p ./build && env-cmd -f ./.env.mixer.production react-scripts build && cp -f CNAME.PROD ./build/CNAME && rm build/robot.txt && yon run patchsw && yon run postbuild",
    "postbuild": "echo 'build done'",
    "postbuild:snap": "react-snap",
    "test": "env-cmd -f ./.env.development.local react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "proxy": "node proxy.js",
    "deploy": "yon run build && env-cmd -f ./.env.staging netlify deploy --prod --dir=build",
    "deploy:prod": "yon run build:prod && env-cmd -f ./.env.production netlify deploy --prod --dir=build",
    "deploy:mixer": "yon run build:mixer && env-cmd -f ./.env.mixer.production netlify deploy --prod --dir=build",
    "i18n": "gstoi18n 1R85easxOUiw184YPJ3wI2-oC-yPyKZi_Xn6a38foQFc ./public/i18n/ -w 0 -w 1 -w 2",
    "patchsw": "cat ./sw.js.append >> ./build/service-worker.js"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.7.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
