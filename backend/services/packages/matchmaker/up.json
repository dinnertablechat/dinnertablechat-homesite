{
  "name": "matchmaker-api",
  "profile": "dinnertable",
  "regions": ["us-east-1"],
  "cors": {
    "enable": true,
    "debug": true,
    "allowed_origins": ["https://dinnertable.chat", "https://test.dinnertable.chat", "*"],
    "allowed_methods": ["HEAD", "GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
    "allowed_headers": ["*"],
    "allow_credentials": true
  },
  "lambda": {
    "memory": 256,
    "policy": [
      {
        "Condition": {"ArnLike":{"AWS:SourceArn":"arn:aws:sns:us-east-1:681274315116:dtc-match-sns"}},
        "Resource": "*",
        "Action": "lambda:invokeFunction",
        "Sid":"snsinvoke",
        "Effect":"Allow"
      }
    ]
  }
}
