{
  "name": "debatehistory",
  "version": "0.0.0",
  "description": "CRUD for debate history",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "jest": "~23.5.0",
    "prettier": "^1.15.2",
    "rimraf": "~2.6.2",
    "ts-jest": "^23.10.4",
    "tsc-watch": "^1.0.30",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tsutils": "^3.4.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "postinstall": "",
    "aws-export": "cp -f ../../../src/aws-exports.js ./src/",
    "startb": "npm run build && npm run start",
    "dev": "PGHOST=localhost PGUSER=app PGDATABASE=dtc PGPASSWORD=null PGPORT=5432 node build/src/main.js",
    "start": "node build/src/main.js",
    "deploy": "npm run build && up && up url",
    "deploylog": "npm run build && up && up url && up logs -f",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc-watch -p tsconfig.release.json --onSuccess 'npm run dev'",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "APLv2",
  "dependencies": {
    "aws-xray-sdk": "^2.0.1",
    "cognito-express": "^2.0.12",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "pg": "^7.6.1",
    "tslib": "~1.9.3"
  }
}
