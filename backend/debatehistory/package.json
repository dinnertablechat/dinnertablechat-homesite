{
  "name": "debatehistory",
  "version": "0.0.0",
  "description": "CRUD for debate history",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.8",
    "@types/node": "^10.12.0",
    "jest": "~23.5.0",
    "prettier": "^1.14.3",
    "rimraf": "~2.6.2",
    "ts-jest": "^23.1.4",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tsutils": "^3.2.0",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "postinstall": "npm run aws-export",
    "aws-export": "cp -f ../../src/aws-exports.js ./src/",
    "startb": "npm run build && npm run start",
    "start": "PGHOST='localhost' PGUSER=dev PGDATABASE=dtc PGPASSWORD=dev PGPORT=5432 node build/src/main.js",
    "deploy": "npm run build && up && up url",
    "clean": "rimraf coverage build tmp",
    "build": "npm run aws-export && tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "APLv2",
  "dependencies": {
    "aws-xray-sdk": "^2.0.1",
    "body-parser": "^1.18.3",
    "cognito-express": "^2.0.12",
    "express": "^4.16.4",
    "pg": "^7.6.0",
    "tslib": "~1.9.3"
  }
}
